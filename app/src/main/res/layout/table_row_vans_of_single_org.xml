<?xml version="1.0" encoding="utf-8"?>


    <TableRow xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/singleOrgVanstableRow"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="#49515c"
        android:paddingTop="5dp"
        android:paddingBottom="5dp">

    <TextView
        android:id="@+id/singleOrgVansSerialNumberTV"
        android:layout_width="75dp"
        android:layout_height="match_parent"
        android:layout_gravity="center_vertical"
        android:fontFamily="sans-serif-light"
        android:gravity="start|center_vertical"
        android:paddingStart="10dp"
        android:text="TextView"
        android:textColor="@color/rowValueColor2"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/singleOrgVansVSITV"
        android:layout_width="175dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center_vertical"
        android:layout_marginStart="5dp"
        android:fontFamily="sans-serif-light"
        android:paddingStart="5dp"
        android:singleLine="false"
        android:text="TextView"
        android:textColor="@color/rowValueColor1"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/singleOrgVansProspectTV"
        android:layout_width="150dp"
        android:layout_height="match_parent"
        android:fontFamily="sans-serif-light"
        android:gravity="center_vertical|end"
        android:text="TextView"
        android:textColor="@color/rowValueColor1"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/singleOrgVansEndTimeTV"
        android:layout_width="175dp"
        android:layout_height="match_parent"
        android:fontFamily="sans-serif-light"
        android:gravity="center_vertical|end"
        android:text="TextView"
        android:textColor="@color/rowValueColor1"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/singleOrgVansSalesOutletTV"
        android:layout_width="150dp"
        android:layout_height="match_parent"
        android:fontFamily="sans-serif-light"
        android:gravity="center_vertical|end"
        android:text="TextView"
        android:textColor="@color/rowValueColor1"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/singleOrgVansQuantityCountTV"
        android:layout_width="150dp"
        android:layout_height="match_parent"
        android:layout_marginRight="15dp"
        android:fontFamily="sans-serif-light"
        android:gravity="center_vertical|end"
        android:text="TextView"
        android:textColor="@color/rowValueColor1"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/singleOrgVansTotalSalesTV"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:background="@color/totalValueBackground"
        android:fontFamily="sans-serif-light"
        android:gravity="center_vertical|end"
        android:text="TextView"
        android:textColor="@color/rowValueColor1"
        android:textSize="20sp" />


    </TableRow>
