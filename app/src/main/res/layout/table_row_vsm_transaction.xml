<?xml version="1.0" encoding="utf-8"?>


<TableRow xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/VSMtableRow"
    android:layout_width="match_parent"
    android:layout_height="40dp"
    android:background="#49515c"
    android:gravity="center_vertical"
    android:paddingTop="5dp"
    android:paddingBottom="5dp">


    <TextView
        android:id="@+id/vsmTransSNtextView"
        android:layout_width="50dp"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-light"
        android:gravity="center_vertical|end"
        android:maxLines="1"
        android:paddingStart="10dp"
        android:paddingEnd="5dp"
        android:text="1"
        android:textColor="@color/rowValueColor3"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/vsmTransVoucherNtxtView"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="20dp"
        android:fontFamily="sans-serif-light"
        android:gravity="center_vertical|end"
        android:maxLines="1"
        android:paddingEnd="5dp"
        android:text="TextView"
        android:textColor="@color/rowValueColor3"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/vsmTransOutletTextView"
        android:layout_width="175dp"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-light"
        android:gravity="start|center_vertical"
        android:maxLines="1"
        android:paddingRight="5dp"
        android:text="TextView"
        android:textColor="@color/rowValueColor4"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/vsmTransTINtextView"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-light"
        android:gravity="center_vertical|end"
        android:maxLines="1"
        android:paddingEnd="5dp"
        android:text="TextView"
        android:textColor="@color/rowValueColor3"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/vsmTransDateNtimeTextV"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-light"
        android:gravity="center_vertical|end"
        android:maxLines="1"
        android:paddingEnd="5dp"
        android:text="TextView"
        android:textColor="@color/rowValueColor3"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/vsmTransItemCountTxtV"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-light"
        android:gravity="center_vertical|end"
        android:maxLines="1"
        android:paddingEnd="5dp"
        android:text="TextView"
        android:textColor="@color/rowValueColor3"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/vsmTransSubTotalTxtv"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-light"
        android:gravity="center_vertical|end"
        android:maxLines="1"
        android:paddingEnd="5dp"
        android:text="TextView"
        android:textColor="@color/rowValueColor3"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/vsmTransVATtextView"
        android:layout_width="125dp"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-light"
        android:gravity="center_vertical|end"
        android:maxLines="1"
        android:paddingEnd="5dp"
        android:text="TextView"
        android:textColor="@color/rowValueColor3"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/vsmTransGrandTotalTextView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:background="#3f4152"
        android:fontFamily="sans-serif-light"
        android:gravity="center_vertical|end"
        android:maxLines="1"
        android:text="TextView"
        android:textColor="@color/rowValueColor3"
        android:textSize="18sp" />

</TableRow>



